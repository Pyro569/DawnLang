function fizzbuzz(a) -> for i = 1 to a do;
    if i % 15 == 0 do write("FizzBuzz") elif i % 5 == 0 do write("Buzz") elif i % 3 == 0 do write("Fizz") else write(i);
    end;

function customfizzbuzz(a, b, c) -> for i = 1 to a do;
    if i % b*c == 0 do write("FizzBuzz") elif i % c == 0 do write("Buzz") elif i % b == 0 do write("Fizz") else write(i);
    end;

function terminalgnome() -> shellrun("gnome-terminal");

function powershell() -> shellrun("powershell");