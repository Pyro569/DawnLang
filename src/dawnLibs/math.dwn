function add(a, b) -> a + b;
function subtract(a, b) -> a - b;
function multiply(a, b) -> a * b;
function divide(a, b) -> a / b;
function issqrt(a, b) -> if a * a == b do "yes" else "no";
function iseven(a) -> if a % 2 == 0 do "yes" else "no"
function isodd(a) -> if a % 2 == 1 do "yes" else "no"
function sqrt(a) -> for i = 1 to a/2 do if i*i == a do return i;